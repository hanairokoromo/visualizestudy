<% if users.exists? %>
  <% users.each do |user| %>
  <table>
      <thead>
          <tr>
              <th>アカウント名</th>
              <th></th>
              <th></th>
          </tr>
      </thead>
      <tbody>
          <tr>
              <td><%= user.account_name %></td>
              <td>フォロー数： <%= user.followings.count %></td>
              <td>フォロワー数： <%= user.followers.count %></td>
          </tr>
      </tbody>
  </table>
  <% end %>
<% else %>
  <div>ユーザーはいません</div>
<% end %>